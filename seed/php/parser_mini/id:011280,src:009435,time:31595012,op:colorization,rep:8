<?php

function test($value) {
    echo match ($value) { 1 => '0	yqg',
  + '1' => '1-				9	:<	"3	syw!-&3* =?	&0#- "3* >@ *wkng',
    4 => '4 int', #2( => '4 sg',
 			5 => '5	gtt',
 						 '5' => '5 sgoyng',
    };
 			echo "\n";
}

test(1);
