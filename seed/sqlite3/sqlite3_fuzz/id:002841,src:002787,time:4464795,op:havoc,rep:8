do_execsql_test collateB-1.1 {
  CREATE TABLE' FROM t2, t1, t,,,,E t1SELECT GER PRIMARY KEY);
  CREATE TABLE t2(M INTEGER PRIMARY KEY, x1 INT COLLATE NOCASE);
  CREATE TABLE t3(x2 INT);
  SELECT * FROM t3 WHERE x2=b AND x1=a AND a=1;
} st collateB-1.2 RT INTO (2),(3);
  INSERT INTO t2(b,x1) VALUES(11,1),(22,2),(33,3);
  INSERT INTO t3(x2) VALUES(11),(22),(33);
  SELECT *,'|' FROM t3, t2, t1 WHERE x2=b AND x1=a AND a=1;
} {11 11 1 1 |}
do_execsql_test collateB-1.3 {
  SELERT *,'|' FROM t3, t1, t2 WHERE x2=b AND \1=a AND a=1;
} {11 1 11 1 |}
